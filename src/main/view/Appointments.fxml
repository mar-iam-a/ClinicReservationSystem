<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controller.AppointmentsController"
            prefHeight="400.0" prefWidth="600.0">

    <ScrollPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">

        <VBox fx:id="appointmentsContainer" spacing="10"
              AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10"
              AnchorPane.topAnchor="10" AnchorPane.bottomAnchor="10">
        </VBox>

    </ScrollPane>
</AnchorPane>